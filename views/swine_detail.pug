extends layout

block content

	div(class='alert alert-success')
		| Swine Registration Number:	
		strong #{data.registration_number} 

	hr
	
	div(class='container container-fluid')

		div(class='col-md-6 float-left')
			div(class='form-group row')
				label(for='registration_number' class='col-form-label col-md-6') Farm Name
				div(class='col-md-6')
					input(id='registration_number' class='form-control-plaintext' type='text' readonly value=data.farm_name)
			div(class='form-group row')
				label(for='swine_breed' class='col-form-label col-md-6') Swine Breed
				div(class='col-md-6')
					input(id='swine_breed' class='form-control-plaintext' type='text' readonly value=data.swine_breed)
			div(class='form-group row')
				label(for='swine_sex' class='col-form-label col-md-6') Swine Sex
				div(class='col-md-6')
					input(id='swine_sex' class='form-control-plaintext' type='text' readonly value=data.swine_sex)
			div(class='form-group row')
				label(for='birthyear' class='col-form-label col-md-6') Birthyear
				div(class='col-md-6')
					input(id='birthyear' class='form-control-plaintext' type='text' readonly value=data.birthyear)
			div(class='form-group row')
				label(for='weight_at_data_collection' class='col-form-label col-md-6') Weight at Data Collection (kg)
				div(class='col-md-6')
					input(id='weight_at_data_collection' class='form-control-plaintext' type='text' readonly value=data.weight_at_data_collection)
			div(class='form-group row')
				label(for='age_at_data_collection' class='col-form-label col-md-6') Age at Data Collection
				div(class='col-md-6')
					input(id='age_at_data_collection' class='form-control-plaintext' type='text' readonly value=data.age_at_data_collection)
			div(class='form-group row')
				label(for='date_registered' class='col-form-label col-md-6') Date Registered
				div(class='col-md-6')
					input(id='date_registered' class='form-control-plaintext' type='text' readonly value=data.date_registered)
			div(class='form-group row')
				label(for='registered_by' class='col-form-label col-md-6') Registered By
				div(class='col-md-6')
					input(id='registered_by' class='form-control-plaintext' type='text' readonly value=data.registered_by)
			div(class='form-group row')
				label(for='standardized_ave_daily_gain' class='col-form-label col-md-6') Standardized Average Daily Gain (g/day)
				div(class='col-md-6')
					input(id='standardized_ave_daily_gain' class='form-control-plaintext' type='text' readonly value=data.standardized_ave_daily_gain)
			div(class='form-group row')
				label(for='actual_ave_daily_gain' class='col-form-label col-md-6') Actual Average Daily Gain (g/day)
				div(class='col-md-6')
					input(id='actual_ave_daily_gain' class='form-control-plaintext' type='text' readonly value=data.actual_ave_daily_gain)
			div(class='form-group row')
				label(for='standardized_backfat_thickness' class='col-form-label col-md-6') Standardized Backfat Thickness (mm)
				div(class='col-md-6')
					input(id='standardized_backfat_thickness' class='form-control-plaintext' type='text' readonly value=data.standardized_backfat_thickness)
			div(class='form-group row')
				label(for='actual_backfat_thickness' class='col-form-label col-md-6') Actual Backfat Thickness (mm)
				div(class='col-md-6')
					input(id='actual_backfat_thickness' class='form-control-plaintext' type='text' readonly value=data.actual_backfat_thickness)
			div(class='form-group row')
				label(for='standardized_feed_efficiency' class='col-form-label col-md-6') Standardized Feed Efficiency (gain/feed)
				div(class='col-md-6')
					input(id='standardized_feed_efficiency' class='form-control-plaintext' type='text' readonly value=data.standardized_feed_efficiency)

		div(class='col-md-6 float-right')
			div(class='form-group row')
				label(for='actual_feed_efficiency' class='col-form-label col-md-6') Actual Feed Efficiency (gain/feed)
				div(class='col-md-6')
					input(id='actual_feed_efficiency' class='form-control-plaintext' type='text' readonly value=data.actual_feed_efficiency)
			div(class='form-group row')
				label(for='birth_weight' class='col-form-label col-md-6') Birth Weight (kg)
				div(class='col-md-6')
					input(id='birth_weight' class='form-control-plaintext' type='text' readonly value=data.birth_weight)
			div(class='form-group row')
				label(for='total_when_born_male' class='col-form-label col-md-6') Total When Born (Male)
				div(class='col-md-6')
					input(id='total_when_born_male' class='form-control-plaintext' type='text' readonly value=data.total_when_born_male)
			div(class='form-group row')
				label(for='total_when_born_female' class='col-form-label col-md-6') Total When Born (Female)
				div(class='col-md-6')
					input(id='total_when_born_female' class='form-control-plaintext' type='text' readonly value=data.total_when_born_female)
			div(class='form-group row')
				label(for='standardized_total_when_born' class='col-form-label col-md-6') Standardized Total When Born
				div(class='col-md-6')
					input(id='standardized_total_when_born' class='form-control-plaintext' type='text' readonly value=data.standardized_total_when_born)
			div(class='form-group row')
				label(for='actual_littersize_born_alive' class='col-form-label col-md-6') Actual Littersize Born Alive
				div(class='col-md-6')
					input(id='actual_littersize_born_alive' class='form-control-plaintext' type='text' readonly value=data.actual_littersize_born_alive)
			div(class='form-group row')
				label(for='standardized_littersize_born_alive' class='col-form-label col-md-6') Standardized Littersize Born Alive
				div(class='col-md-6')
					input(id='standardized_littersize_born_alive' class='form-control-plaintext' type='text' readonly value=data.standardized_littersize_born_alive)
			div(class='form-group row')
				label(for='parity' class='col-form-label col-md-6') Parity
				div(class='col-md-6')
					input(id='parity' class='form-control-plaintext' type='text' readonly value=data.parity)
			div(class='form-group row')
				label(for='standardized_littersize_at_weaning' class='col-form-label col-md-6') Standardized Littersize At Weaning
				div(class='col-md-6')
					input(id='standardized_littersize_at_weaning' class='form-control-plaintext' type='text' readonly value=data.standardized_littersize_at_weaning)
			div(class='form-group row')
				label(for='actual_littersize_at_weaning' class='col-form-label col-md-6') Actual Littersize at Weaning
				div(class='col-md-6')
					input(id='actual_littersize_at_weaning' class='form-control-plaintext' type='text' readonly value=data.actual_littersize_at_weaning)
			div(class='form-group row')
				label(for='litterweight_at_weaning' class='col-form-label col-md-6') Litterweight at Weaning
				div(class='col-md-6')
					input(id='litterweight_at_weaning' class='form-control-plaintext' type='text' readonly value=data.litterweight_at_weaning)
			div(class='form-group row')
				label(for='maternal' class='col-form-label col-md-6') Maternal Registration Number
				div(class='col-md-6')
					input(id='maternal' class='form-control-plaintext' type='text' readonly value=data.maternal)
			div(class='form-group row')
				label(for='paternal' class='col-form-label col-md-6') Paternal Registration Number
				div(class='col-md-6')
					input(id='paternal' class='form-control-plaintext' type='text' readonly value=data.paternal)
		hr
		br